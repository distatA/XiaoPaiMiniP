/* pages/index/ardess-detalist/ardess-detalist.wxss */

page {
  background: #f2f2f2;
}

.ardess-header {
  height: 90rpx;
  background: #fff;
  font-size: 32rpx;
  line-height: 90rpx;
  padding-left: 40rpx;
}

.content {
  padding: 20rpx 40rpx;
}

.vis-history-title {
  font-size: 24rpx;
  padding: 5rpx 0;
}

.vis-history-city {
  display: flex;
  flex-wrap: wrap;
  padding-left: 15rpx;
}

.city-box{
  font-size: 30rpx;
  padding: 10rpx 30rpx;
  background: #fff;
  margin: 15rpx;
  border-radius: 10rpx;
}

.check{
  font-size: 34rpx;
  color: #ccc;
}