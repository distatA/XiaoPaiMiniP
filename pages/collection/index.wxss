page {
    background: #fafafa;
}

.nav {
    display: flex;
    height: 85rpx;
    justify-content: space-around;
    background: #22d9d9;
    line-height: 85rpx;
}

.school-title {
    font-family: PingFang-SC-Bold;
    font-size: 28rpx;
    color: #fff;
}

.card-box {
    width: 100%;
    position: relative;
    margin: 20rpx 0;
    overflow: hidden;
}

.school-card {
    width: 97%;
    height: 136rpx;
    background: #fff;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.2);
    padding: 25rpx 0rpx 25rpx 25rpx;
    display: flex;
    position: absolute;
    top: 0;
    left: 0rpx;
}

.school-card2 {
    width: 70%;
    background: #fff;
    height: 136rpx;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.2);
    padding: 25rpx 0rpx 25rpx 25rpx;
    display: flex;
    position: absolute;
    top: 0;
    left: -70rpx;
}

.delet {
    width: 97%;
    height: 136rpx;
    background: #43c7b7;
    padding: 25rpx 0rpx 25rpx 25rpx;
}

.left-logo {
    width: 114rpx;
    height: 114rpx;
    margin-top: 15rpx;
}

.left-logo image {
    width: 100%;
    height: 100%;
}

.card-center {
    padding-left: 20rpx;
}

.school-name {
    font-size: 30rpx;
    color: #4d4d4d;
    margin-top: 30rpx;

}

.comprehensive {
    font-size: 22rpx;
    color: #878787;
}

.comprehensive text {
    padding-left: 10rpx;
}

.comprehensive text:first-child {
    padding-left: 0;
}

.comprehensive text:last-child {
    padding-left: 0;
}

.enrolment {
    font-family: PingFang-SC-Regular;
    font-size: 20rpx;
    color: #878787;
    padding-top: 10rpx;
}

.plan-foot {
    display: flex;
}

.plan-foot view:nth-child(2) {
    padding-left: 10rpx;
}

.enrolment text {
    font-family: PingFang-SC-Regular;
    font-size: 20rpx;
    color: #353535;
}

.delet-logo {
    float: right;
    display: flex;
}

.delet-logo view {
    font-size: 70rpx;
    color: #fff;
    padding-top: 30rpx;
    padding-right: 35rpx;
}

.more {
    font-family: PingFang-SC-Bold;
    font-size: 20rpx;
    line-height: 32rpx;
    color: #44c6b8;
    text-align: center;
}

.newsbox {
    width: 90%;
    margin: 20rpx auto;
    padding: 20rpx 20rpx 20rpx;
    background-color: #fff;
    box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.1);
}

.newsbox .img {
    display: inline-block;
    width: 28.5%;
    height: 200rpx;
}

.newsbox image {
    width: 100%;
    height: 100%;
}

.newsbox .info {
    display: inline-block;
    width: 68%;
    margin-left: 3.5%;
    height: 200rpx;
    overflow: hidden;
    font-size: 32rpx;
    position: relative;
}

.newsbox .info .tit {
    font-family: PingFang-SC-Regular;
    font-size: 30rpx;
    color: #5a5a5a;
    padding-top: 10rpx;
}

.status {
    position: absolute;
    bottom: 10rpx;
    width: 100%;
}

.status>view {
    display: inline-block;
    width: 33.33%;
    line-height: 60rpx;
    color: #878787;
}

.status .iconfont {
    margin-right: 6rpx;
    color: #cdcdcd;
}

.newsicon {
    float: left;
    font-family: AdobeHeitiStd-Regular;
    font-size: 55rpx;
    color: #878787;
}

.xingxing {
    float: left;
    font-family: AdobeHeitiStd-Regular;
    font-size: 45rpx;
    color: #878787;
}

.pinglun {
    float: left;
    font-family: AdobeHeitiStd-Regular;
    font-size: 45rpx;
    color: #878787;
}

.click {
    font-family: AdobeHeitiStd-Regular;
    font-size: 26rpx;
    color: #878787;
}

.comment {
    font-family: AdobeHeitiStd-Regular;
    font-size: 26rpx;
    color: #878787;
}

.xing {
    font-family: AdobeHeitiStd-Regular;
    font-size: 26rpx;
    color: #878787;
}

.class {
    margin-bottom: 30rpx;
}

.class-card {
    width: 90%;
    margin: 20rpx auto;
    background: #fff;
    box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.2);
    display: flex;
    padding: 15rpx;
}

.card-left-logo {
    width: 205rpx;
    height: 170rpx;
}

.card-left-logo image {
    width: 100%;
