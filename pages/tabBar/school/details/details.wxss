  color: #333;
}

.s_summery {
  padding: 40rpx 30rpx;
}

.s_summery_title {
  display: flex;
  justify-content: space-between;
  font-size: 40rpx;
}

.jiantou {
  font-size: 55rpx;
  color: #ccc;
}

.s_summery_content {
  font-family: PingFang-SC-Regular;
  font-size: 30rpx;
  color: #595959;
  margin-top: 20rpx;
}

.s_syle {
  padding: 30rpx 10rpx;
  border-bottom: 1px solid #ccc;
}
