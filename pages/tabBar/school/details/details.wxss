}

.tszy {
  border: #ccc 1px solid;
}

.special_tit {
  font-size: 34rpx;
  padding: 20rpx 20rpx 10rpx 20rpx;
}

.special_box {
  font-size: 26rpx;
  line-height: 40rpx;
}

.bold {
  font-weight: 500;
}

.xiala {
  width: 26rpx;
  height: 26rpx;
  border-radius: 26rpx;
  line-height: 30rpx;
  margin-top: 24rpx;
  border: #ccc 1px solid;
  text-align: center;
  margin-right: 10rpx;
  font-size: 20rpx;
}


.screen {
  width: 100%;
  height: 50rpx;
  font-size: 26rpx;
  line-height: 50rpx;
  display: flex;
}



.select {
  position: absolute;
  top: 50rpx;
  left: -18rpx;
  width: 100rpx;
  line-height: 48rpx;
  overflow: hidden;
  border: #ccc 2rpx solid;
  border-bottom: none;
  z-index: 9999;
  background: #fff;
  text-align: center;
}

.select view {
  border-bottom: #ccc 2rpx solid;
  height: 48rpx;
  width: 100%;
}

.showSuccess {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  z-index: 1001;
}

.showSuccess .showSuccess-main {
  width: 470rpx;
  height: 375rpx;
  position: fixed;
  left: 0;
  right: 0;
  top: 25%;
  margin: 0 auto;
  z-index: 1003;
}

.showSuccess .big_bg {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 1002;
}

.showSuccess image {
  width: 175rpx;
  height: 175rpx;
  display: block;
  padding-top: 10rpx;
  margin: 0 auto;
}

.showSuccess image.close {
  width: 70rpx!important;
  height: 70rpx!important;
  position: absolute;
  right: 0;
  top:-8rpx;
  margin: 0;
}

.showSuccess .content {
  height: 380rpx;
  width: 470rpx;
  border-radius: 20rpx;
  position: relative;
  background: #fff;
}

.showSuccess .btn {
  background: #fff;
  width: 470rpx;
  height: 70rpx;
  line-height: 70rpx;
}

.showSuccess .guanzhuggBtn {
    line-height: 30rpx;
}
.showSuccess .guanzhuggBtn > view:nth-child(2) {
    border-left: 0!important;
}
.showSuccess .flex {
  padding: 10rpx 0;
  background: #fff;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.showSuccess .flex view:nth-child(2) {
  border-left: 1px solid #ccc;
}

.mask {
  position: fixed;
  bottom: 0;
}

.vr-text-tip {
  position: absolute;
  bottom: 10rpx;
  left: 10rpx;
  z-index: 9;
  color: #fff;
  font-size: 28rpx;
  width: 170rpx;
  height: 40rpx;
  background: rgba(0, 0, 0, 0.4);
  padding: 0 10rpx;
  line-height: 40rpx;
  border-radius: 10rpx;
}

.flaot-guanzhu {
    position: fixed;
    top: 40rpx;
    left: 20rpx;
    right: 20rpx;
    background: #000000;
    z-index: 100000;
}

.flaot-guanzhu .close {
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  width: 50rpx;
  height: 50rpx;
}

.flaot-guanzhu .close image {
  width: 50rpx;
  height: 50rpx;
}

.tishi {
  position: absolute;
  top: 0rpx;
  right: 50rpx;
  width: 300rpx;
  height: 50rpx;
  font-size:28rpx;
  color:#000;
}

.blue2 {
    color:#74dbd3;
    display: inline-block;
}
