.main {
    width: 100%;
    height: 100%;
    padding: 40rpx 30rpx;
    box-sizing: border-box;
}

.title {
    font-size: 36rpx;
    font-weight: bold;
    text-align: center;
}

.content {
    margin-top: 40rpx;
}

.download {
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    background: #22d8d9;
    color: #fff;
    margin: 20rpx auto;
    border-radius: 35rpx;
    font-size: 32rpx;
}

._noPost {
    text-align: center;
    position: relative;
}

.no-data {
    width: 200rpx;
    height: 230rpx;
    position: absolute;
    top: 350rpx;
    left: 0;
    right: 0;
    margin: auto;
}
