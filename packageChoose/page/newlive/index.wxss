.boxInfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 20rpx
}
.itemlist>image{
  width: 750rpx;
  height: 240rpx;
}
.itemlist{
  position: relative;
  margin-top: 32rpx
}

.totalPeople_1,
.totalPeople_2,
.totalPeople_3,
.totalPeople_4,
.totalPeople_5,
.totalPeople_6{
  /* width: 145rpx; */
  height:34rpx;
  display: flex;
  align-items: center;
  justify-content: center;

  border-radius:17rpx;

  position: absolute;
  bottom: 26rpx;
  right: 24rpx;
  padding: 0 15rpx 0 15rpx
  
}
.totalPeople_1{
  background:rgba(255,177,101,1);
}
.totalPeople_2{
background:rgba(17,177,161,1);
}
.totalPeople_3{
background:rgba(43,36,218,1);
}
.totalPeople_4{
background:rgba(245,153,33,1);
}
.totalPeople_5{
background:rgba(80,144,240,1);
}
.totalPeople_6{
background:rgba(172,163,255,1);
}
.itemlist text{
height:17rpx;
font-size:16rpx;
font-family:PingFang SC;
font-weight:bold;
line-height:17rpx;
color:rgba(255,255,255,1);
opacity:1;

}